{
  "name": "next-temp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "next build",
    "build:turbo": "next build --turbopack",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "pre-push": "npm run qa",
    "qa": "npm run qa:format && npm run qa:lint && npm run qa:type-check && npm run qa:test",
    "qa:format": "prettier --check .",
    "qa:lint": "eslint .",
    "qa:type-check": "tsc --noEmit",
    "qa:test": "vitest run",
    "security:ci": "npm audit --audit-level=high --production || true",
    "ci": "npm run qa && npm run build"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
